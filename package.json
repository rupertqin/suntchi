{
  "name": "suntchi",
  "version": "0.1.1",
  "author": "Qin",
  "scripts": {
    "start": "node bin/development.js",
    "production": "node bin/production.js",
    "debug": "npm run clean && node bin/debug",
    "build": "npm test && npm run clean && npm run compile",
    "compile": "babel src/ --out-dir app/ --retain-lines --source-maps",
    "clean": "rm -rf app/",
    "test": "mocha -u bdd --compilers js:babel-core/register",
    "dev": "npm start & npm run fe & npm run browserSync",
    "fe": "webpack --watch",
    "browserSync": "browser-sync start --proxy 'localhost:3003' --no-open --reload-delay 300 --files 'src/' 'public/' "
  },
  "dependencies": {
    "co": "^4.6.0",
    "debug": "^2.2.0",
    "ejs": "^2.4.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.0.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^1.1.1",
    "koa-logger": "^1.3.0",
    "koa-onerror": "^1.3.1",
    "koa-router": "^7.0.0",
    "koa-static-plus": "^0.1.1",
    "koa-views": "^5.0.1",
    "lodash": "^4.11.1",
    "mongoose": "^4.4.19",
    "semantic-ui": "^2.1.8"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "colors": "^1.1.2",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-config-standard": "^5.2.0",
    "eslint-plugin-promise": "^1.3.1",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^2.4.5",
    "node-sass": "^3.3.3",
    "postcss-cli": "^2.5.2",
    "postcss-import": "^8.1.2",
    "postcss-partial-import": "^1.3.0",
    "postcss-scss": "^0.1.8",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^2.0.1",
    "should": "^8.3.0",
    "style-loader": "^0.12.4",
    "supertest": "^1.2.0",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0"
  }
}
